{
  "name": "no-adb-killer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clear": "echo \"üîÑ Clearing\"; rm -rf app dist",
    "test": "run-s -s build mocha",
    "start": "npm-watch",
    "build": "run-s -s clear build:scripts launch-builder build:extension",
    "build:scripts": "echo \"üîπ Building scripts\"; webpack --display=errors-only --config webpack/webpack-scripts-build.js --mode=production",
    "build:extension": "echo \"üî∑ Building extension\"; webpack --display=errors-only --config webpack/webpack-extension-build.js --mode=production",
    "start:scripts": "echo \"#Ô∏è‚É£  Starting scripts\"; webpack --display=errors-only --config webpack/webpack-scripts-build.js --mode=development",
    "start:extension": "echo \"*Ô∏è‚É£  Starting extension\"; webpack --display=errors-only --config webpack/webpack-extension-build.js --mode=development",
    "start:watch": "run-s -s clear start:scripts launch-builder start:extension",
    "launch-builder": "echo \"üîµ Launching builder\"; node dist/build",
    "mocha": "echo \"üïë Testing\"; mocha dist/test.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "watch": {
    "start:watch": {
      "patterns": ["static", "src", "test"],
      "extensions": [".ts", ".json", ".js"],
      "ignore": ["static/config.build.json"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasmrdt/no-adb-killer.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lucasmrdt/no-adb-killer/issues"
  },
  "homepage": "https://github.com/lucasmrdt/no-adb-killer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chrome": "0.0.85",
    "@types/diff": "^4.0.2",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.2",
    "@types/prompts": "^2.4.0",
    "@types/webpack": "^4.4.31",
    "colors": "^1.3.3",
    "copy-webpack-plugin": "^5.0.3",
    "diff": "^4.0.1",
    "fast-diff": "^1.2.0",
    "husky": "^2.3.0",
    "mocha": "^6.1.4",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.6.0",
    "prompts": "^2.0.4",
    "simple-git": "^1.113.0",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-conditional-loader": "^1.0.12",
    "webpack-merge": "^4.2.1"
  }
