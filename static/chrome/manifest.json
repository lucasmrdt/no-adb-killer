{
  "name": "No AAdblock-Killer",
  "short_name": "no-adb-killer",
  "version": "1.1",
  "description": "Avoid abd killer, enjoy website without advertisement again!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "scripts/*"
  ],
  "permissions": [
    "webRequest",
    "*://*/",
    "webRequestBlocking"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
