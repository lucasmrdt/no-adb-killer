{
  "name": "No Adblock-Killer",
  "short_name": "no-adb-killer",
  "version": "1.6",
  "description": "Avoid the Adblock killers, enjoy the ad-free website again!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    "scripts/*"
  ],
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.6play.fr/*",
    "https://*.tf1.fr/*"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
